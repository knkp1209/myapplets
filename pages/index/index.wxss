/**index.wxss**/
/* 轮播图 */
.swiper-item{
  display: block;
}
.slide-image {
  width: 100%;
}
swiper {
  width: 100%;
  height: 230px;
}
/* 分类 */
.category {
  padding: 15px 0 10px 0;
  overflow: auto;
  background-color: #FFF;
  margin-bottom: 10px;
}
.category_item {
  width: 25%;
  float: left;
  margin-bottom: 10px;
}
.icon_wrap {
  width: 40px;
  margin: 0 auto;
  margin-bottom: 5px;
}
.index_icon {
  width: 40px;
  height: 40px;
}
.category_item_text {
  text-align: center;
  font-size: 13px;
  color: #ab956c;
}
/* 标题 */
.biaoti{
  text-align: center;
  font-size: 18px;
  font-weight: 600px;
  color: #ab956c;
}
/* 预约信息 */
.cp{
  width: 99.5%;
  height: 100%;
  border: 1px solid red;
  margin-top: 10rpx;
  
}
.l1{
  width: 49.4%;
  height: 250rpx;
  border: 1px solid red;
  float: left;
  background-color: #c6bfad;
  
}
.l2{
  width: 49.4%;
  height: 250rpx;
  border: 1px solid red;
  float: right;
  background-color: #c4c4c4;
}