/**index.wxss**/
/* 轮播图 */
.swiper-item{
  display: block;
}
.slide-image {
  width: 100%;
}
swiper {
  width: 100%;
  height: 230px;
}
/* 分类 */
.category {
  padding: 15px 0 10px 0;
  overflow: auto;
  background-color: #FFF;
  margin-bottom: 10px;
}
.category_item {
  width: 25%;
  float: left;
  margin-bottom: 10px;
}
.icon_wrap {
  width: 40px;
  margin: 0 auto;
  margin-bottom: 5px;
}
.index_icon {
  width: 40px;
  height: 40px;
}
.category_item_text {
  text-align: center;
  font-size: 13px;
  color: #ab956c;
}
/* 标题 */
.biaoti{
  height: 100rpx;
  margin-top: 50rpx;
  text-align: center;
  font-size: 1em;
  font-weight: 600px;
  color: #ab956c;
  font-family: "微软雅黑";
}
/* 预约信息 */
.cp{
  width: 100%;
  height: 300rpx;
  /* border: 1rpx solid red; */
  margin-top: 10rpx;
  overflow: hidden;
}
.l1{
  width: 49.9%;
  /* border: 1rpx solid red; */
  float: left;
  background-color: #c6bfad;
  box-sizing: border-box;
}
.l2{
  width: 49.9%;
  /* border: 1rpx solid red; */
  float: right;
  background-color: #c4c4c4;
  box-sizing: border-box;
}
.cp1{
  width: 100%;
  height: 250rpx;
  /* border: 1rpx solid red; */
  margin-top: 5rpx;
}
.l3{
  width: 100%;
  height: 100%;
  /* border: 1rpx solid red; */
  background-color: #dddddd;
}
/* 所有产品 */
.allcp{
  width: 99.6%;
  /* border: 1rpx solid red; */
  height: 100%;
  text-align: center;
}
.acp1{
  width: 49.5%;
  height: 300rpx;
  /* border: 1rpx solid red; */
  background-color: #f7f7f7;
  float: left;
  box-sizing: border-box;
  border-radius: 5%;
  margin-bottom: 10rpx;
}
.acp2{
  width: 49.5%;
  height: 300rpx;
  /* border: 1rpx solid red; */
  background-color: #f7f7f7;
  float: right;
  box-sizing: border-box;
  border-radius: 5%;
  margin-bottom: 10rpx;
}